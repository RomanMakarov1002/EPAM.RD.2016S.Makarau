<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ClientDefaults" type="System.Configuration.NameValueSectionHandler"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <ClientDefaults>
    <add key="NumberOfInstances" value="4"/>
  </ClientDefaults>
  <appSettings>
    <add key="XmlFilePath" value="1.xml"/>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true"></trace>
    <sources>
      <source name ="CustomSource" switchName="BooleanSwitch" >
        <listeners>
          <add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener" />
          <remove name ="Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="BooleanSwitch" value="Information"  />
    </switches>
  </system.diagnostics>
</configuration>